// MOOS file

ServerHost = localhost
ServerPort = 9000

Community  = AUV

//Serial port configuration
GPS_SERIAL_PORT = /dev/ttyUSB1

//------------------------------------------
// Antler configuration  block
ProcessConfig = ANTLER
{
  MSBetweenLaunches = 200

  Run = MOOSDB       	@ NewConsole = true
  Run = uMACView       @ NewConsole = false
  Run = uMS             @ NewConsole=false
  Run = iGPS           @ NewConsole = false
  Run = pTrustGPS       @ NewConsole = false
}

//------------------------------------------
// Global Param

LatOrigin    = 48.418079
LongOrigin   = -4.472279

//------------------------------------------------
// iGPS config block

ProcessConfig = iGPS
{
  AppTick   = 5
  CommsTick = 5

  UART_BAUD_RATE = 4800
}

//------------------------------------------------
// pTrustGPS config block

ProcessConfig = pTrustGPS
{
   AppTick   = 4
   CommsTick = 4

   ALTITUDE_AT_GPS_SUBMERGED = -0.5
   PARANOIA_LEVEL = 10
}
ProcessConfig = uMACView
{
   AppTick   = 4
   CommsTick = 4
}

