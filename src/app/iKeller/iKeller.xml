     <documentation>
        <synopsis>
          Read the range value from the echo-sounder tritech sensor.
        </synopsis>
        <interface>
           <subscriptions>
              <moosvar>
                 <varname>GET_KELLER_TEMPERATURE</varname>
                 <vartype>bool</vartype>
                 <varinfo>description of the MOOSVar</varinfo>
              </moosvar>
              <moosvar>
                 <varname>RESET_KELLER_PRESSURE</varname>
                 <vartype>double</vartype>
                 <varinfo>Reset zero to match current pressure</varinfo>
              </moosvar>
              <moosvar>
                 <varname>SET_ZERO_KELLER_PRESSURE</varname>
                 <vartype>double</vartype>
                 <varinfo>Reset zero to a given value</varinfo>
              </moosvar>
           </subscriptions>
           <publications>
              <moosvar>
                 <varname>KELLER_DEPTH</varname>
                 <vartype>double</vartype>
                 <varinfo>the temperature value, read from the sensor</varinfo>
              </moosvar>
              <moosvar>
                 <varname>KELLER_TEMPERATURE</varname>
                 <vartype>double</vartype>
                 <varinfo>the temperature value, read from the sensor</varinfo>
              </moosvar>
           </publications>
        </interface>
        <!-- if there is a bug or a special fact to know.. -->
        <optional-comments>
           No comment.
        </optional-comments>
        <!-- if something can be improved -->
        <suggested-improvements>
        </suggested-improvements>
     </documentation>

  </moosapp>
