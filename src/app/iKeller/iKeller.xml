<xmldoc>
  <moosapp>

     <info>
        <name>iKeller</name>
        <organization>Toutatis AUVs - ENSTA Bretagne</organization>
        <date>2015</date>
        <authors>
           <author>Clement Aubry</author>
        </authors>
        <licence>MIT</licence>
